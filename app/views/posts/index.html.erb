<div class="container">
    <div class="row">
        <div class="col-lg-12 text-center">
            <h1 class="mt-5">Namma Instagram</h1>
        </div>
    </div>
    
    <% @posts.each do |x| %>
    <div class="white">
        <h1>
            <%= x.description %>
        </h1>
        <%= image_tag(x.image.url(:medium))%>
        
    </div>
    <% end %>
</div>